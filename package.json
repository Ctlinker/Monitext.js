{
  "name": "project-monitext",
  "version": "0.0.0",
  "description": "Monitext aim to be an all in one monitoring solution for js based applications. It provides a simple and easy to use interface to monitor your applications and get notified when something goes wrong.",
  "private": "true",
  "scripts": {
    "scripts": "pnpm mtxt-scripts",
    "test": "pnpm vitest",
    "format": "prettier --write 'packages/**/*.{js,ts,json,md}'",
    "check-format": "prettier --check 'packages/**/*.{js,ts,json,md}'",
    "changeset": "changeset",
    "syncpack": "syncpack",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "monitext",
    "logging",
    "monitoring",
    "observability",
    "javascript",
    "typescript",
    "nodejs",
    "bun",
    "deno",
    "cli",
    "sdk",
    "devtools",
    "developer-tools",
    "log-management",
    "alerting",
    "notifications",
    "dashboard",
    "analytics",
    "system-monitoring",
    "error-tracking",
    "runtime-logging"
  ],
  "author": "Ctlinker <linkeris404@gmail.com>",
  "license": "Apache-2.0",
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@monitext-dev/builder": "workspace:0.0.1",
    "@monitext-dev/paths": "workspace:1.0.0",
    "@monitext-dev/scripts": "workspace:1.0.0",
    "@monitext-dev/tsconfig": "workspace:1.0.0",
    "@types/bun": "^1.3.1",
    "@types/deno": "^2.5.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "prettier": "^3.6.2",
    "syncpack": "^13.0.4",
    "tsx": "4.20.5",
    "typescript": "^5.9.2",
    "vitest": "3.1.2"
  },
  "dependencies": {
    "vitepress": "^1.6.4",
    "vue": "^3.5.24"
  }
}